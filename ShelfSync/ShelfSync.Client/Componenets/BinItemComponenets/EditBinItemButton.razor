@rendermode RenderMode.InteractiveAuto
@inject NavigationManager NavigationManager


 <button @onclick="EditBinItem" class="btn btn-sm btn-outline-primary me-1"> <i class="fas fa-edit"></i> Edit</button>


@code {
    [Parameter]
    public int Id {get;set;}

    void EditBinItem(){
        NavigationManager.NavigateTo($"/editBinItem/{Id}");
    }

}
